#!/usr/bin/env python
import sys
    if not '' in sys.path:
sys.path.insert(0, '')
from celery.bin.celeryd import run_worker, parse_options

def main():
    import multiprocessing
    multiprocessing.freeze_support()
    options = parse_options(sys.argv[1:])
    run_worker(**vars(options))

if __name__ == "__main__":
    main()
